{
  "name": "deckgl-typescript-example",
  "version": "1.0.0",
  "description": "Example of consuming deck.gl with typescript and typings",
  "private": true,
  "main": "index.js",
  "scripts": {
    "transpile": "tsc -p .",
    "bundle": "browserify ./dist/es6/index.js -o ./dist/bundle.js -t babelify",
    "build": "npm run transpile && npm run bundle"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react-dom": "^16.0.6",
    "deck.gl": "^5.3.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "deckgl-typings": "git+https://github.com/danmarshall/deckgl-typings.git",
    "typescript": "^2.9.2"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
